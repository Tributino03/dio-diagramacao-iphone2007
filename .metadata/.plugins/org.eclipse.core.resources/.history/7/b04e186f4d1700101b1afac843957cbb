package iphone;

public class Main {
    public static void main(String[] args) {
        Iphone2007 iphone = new Iphone2007();

        System.out.println(iphone.ligarIphone());
        System.out.println(iphone.desligarIphone());

        // Teste do Reprodutor Musical
        System.out.println(iphone.selecionarMusica("Let It Be"));
        System.out.println(iphone.tocar());
        System.out.println(iphone.pausar());

        // Teste do Aparelho Telef√¥nico
        System.out.println(iphone.ligar("1234-5678"));
        System.out.println(iphone.atender());
        System.out.println(iphone.iniciarCorreioVoz());

        // Teste do Navegador na Internet
        System.out.println(iphone.exibirPagina("https://www.apple.com"));
        System.out.println(iphone.adicionarNovaAba());
        System.out.println(iphone.atualizarPagina());
    }
}
